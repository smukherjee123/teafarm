<%- model_class = Irriset -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:setname) %>:</strong></dt>
  <dd><%= @irriset.setname %></dd>
  <dt><strong><%= model_class.human_attribute_name(:motordet) %>:</strong></dt>
  <dd><%= @irriset.motordet %></dd>
  <dt><strong><%= model_class.human_attribute_name(:motorhp) %>:</strong></dt>
  <dd><%= @irriset.motorhp %></dd>
  <dt><strong><%= model_class.human_attribute_name(:gpm) %>:</strong></dt>
  <dd><%= @irriset.gpm %></dd>
  <dt><strong><%= model_class.human_attribute_name(:fuel) %>:</strong></dt>
  <dd><%= @irriset.fuel %></dd>
  <dt><strong><%= model_class.human_attribute_name(:fuelperhr) %>:</strong></dt>
  <dd><%= @irriset.fuelperhr %></dd>
  <dt><strong><%= model_class.human_attribute_name(:location_id) %>:</strong></dt>
  <dd><%= @irriset.location_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              irrisets_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_irriset_path(@irriset), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              irriset_path(@irriset),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
